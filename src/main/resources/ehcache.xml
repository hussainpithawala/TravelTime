<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true"
	monitoring="autodetect" dynamicConfig="true">
	<diskStore path="java.io.tmpdir" />
	<cache name="airportsCache" maxEntriesLocalHeap="1000"
		maxEntriesLocalDisk="100" eternal="false" diskSpoolBufferSizeMB="5"
		timeToIdleSeconds="300" timeToLiveSeconds="600"
		memoryStoreEvictionPolicy="LFU" transactionalMode="off">
		<persistence strategy="localTempSwap" />
	</cache>
	<cache name="timezonesCache" maxEntriesLocalHeap="12000"
		maxEntriesLocalDisk="1000" eternal="false" diskSpoolBufferSizeMB="25"
		timeToIdleSeconds="300" timeToLiveSeconds="600"
		memoryStoreEvictionPolicy="LFU" transactionalMode="off">
		<persistence strategy="localTempSwap" />
	</cache>
</ehcache>